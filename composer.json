{
    "name": "bogddan/column-sortable",
    "description": "Package for handling column sorting in Laravel 6.x",
    "keywords": [
        "sortable",
        "sorting",
        "column",
        "sort",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Martin Kiesel",
            "email": "martin.kiesel@gmail.com",
            "role": "Developer and maintainer"
        }
    ],
    "require": {
        "php": "8.2",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8..5"
    },
    "autoload": {
        "psr-4": {
            "Bogddan\\ColumnSortable\\": "src/ColumnSortable/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bogddan\\ColumnSortable\\ColumnSortableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
